{
  "short_name": "Storage Viwer",
  "name": "Storage Viewer",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "version": "1.0.0",
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": ["tabs", "storage", "scripting"],
  "host_permissions": ["<all_urls>"]
}
